# mongodb配置
MONGO_VERSION=latest
MONGO_HOST_PORT=27017
MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=123456
MONGO_DATA_DIR=./services/mongodb/data

# redis配置
REDIS_VERSION=latest
REDIS_HOST_PORT=6379
