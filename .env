# 配置一些环境变量，用于docker-compose.yml中使用

# 服务目录
SERVICES_DIR=./services

# 网站目录
WWWROOT_DIR=./wwwroot

# 日志目录
LOGS_DIR=./logs

# 仓库地址
REPOSITORIES=mirrors.aliyun.com
REPOSITORIES2=mirrors.ustc.edu.cn
REPOSITORIES3=mirror.tuna.tsinghua.edu.cn

# 时区
TZ=Asia/Shanghai

# nginx配置
NGINX_VERSION=1.17.8-alpine
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443
NGINX_CONFD_DIR=./services/nginx/conf.d
NGINX_CONF_FILE=./services/nginx/nginx.conf
NGINX_FASTCGI_PHP_CONF=./services/nginx/fastcgi-php.conf
NGINX_FASTCGI_PARAMS=./services/nginx/fastcgi_params
NGINX_SSL_CERTIFICATE_DIR=./services/nginx/ssl
NGINX_LOG_DIR=./logs/nginx

# php74配置
PHP74_VERSION=7.4.3
PHP74_FASTCGI_PORT=9000
PHP74_PHP_CONF_FILE=./services/php/php74/php.ini
PHP74_FPM_CONF_FILE=./services/php/php74/php-fpm.conf
PHP74_FPMD_DIR=./services/php/php74/php-fpm.d
PHP74_LOG_DIR=./logs/php74

# php73配置
PHP73_VERSION=7.3.15
PHP73_FASTCGI_PORT=9000
PHP73_PHP_CONF_FILE=./services/php/php73/php.ini
PHP73_FPM_CONF_FILE=./services/php/php73/php-fpm.conf
PHP73_FPMD_DIR=./services/php/php73/php-fpm.d
PHP73_LOG_DIR=./logs/php73

# php72配置
PHP72_VERSION=7.2.28
PHP72_FASTCGI_PORT=9000
PHP72_PHP_CONF_FILE=./services/php/php72/php.ini
PHP72_FPM_CONF_FILE=./services/php/php72/php-fpm.conf
PHP72_FPMD_DIR=./services/php/php72/php-fpm.d
PHP72_LOG_DIR=./logs/php72

# php56配置
PHP56_VERSION=5.6.40
PHP56_FASTCGI_PORT=9000
PHP56_PHP_CONF_FILE=./services/php/php56/php.ini
PHP56_FPM_CONF_FILE=./services/php/php56/php-fpm.conf
PHP56_FPMD_DIR=./services/php/php56/php-fpm.d
PHP56_LOG_DIR=./logs/php56

