# memcached配置
MEMCACHED_VERSION=latest
MEMCACHED_HOST_PORT=11211
MEMCACHED_CACHE_SIZE=256

# phpmyadmin配置
PHPMYADMIN_HOST_PORT=8080

# phpredisadmin配置
PHPREDISADMIN_HOST_PORT=8081

# mongo-express配置
MONGOEXPRESS_HOST_PORT=8082

# adminmongo配置
ADMINMONGO_HOST_PORT=1234

# rabbitmq配置
RABBITMQ_VERSION=latest
RABBITMQ_HOST_PORT_CLIENT=5672
RABBITMQ_HOST_PORT_SERVER=15672
RABBITMQ_DEFAULT_USER=root
RABBITMQ_DEFAULT_PASS=123456